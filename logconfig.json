{
    "version": 1,
    "disable_existing_loggers": true,
    "formatters": {
        "standard": {
            "format": "%(asctime)s [%(levelname)s] %(name)s(%(threadName)s): %(message)s",
            "datefmt": "%Y-%m-%d %H:%M:%S"
        }
    },
    "handlers": {
        "default": {
            "level": "DEBUG",
            "formatter": "standard",
            "class": "logging.StreamHandler"
        }
    },
    "loggers": {
        "": {
            "handlers": [
                "default"
            ],
            "level": "DEBUG",
            "propagate": false
        },
        "composant": {
            "handlers": [
                "default"
            ],
            "level": "INFO",
            "propagate": false
        },
        "werkzeug": {
            "handlers": [
                "default"
            ],
            "level": "WARNING",
            "propagate": false
        },
        "__main__": {
            "handlers": [
                "default"
            ],
            "level": "INFO",
            "propagate": false
        }
    }
}